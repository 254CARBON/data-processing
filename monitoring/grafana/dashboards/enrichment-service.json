{
  "dashboard": {
    "id": null,
    "title": "Enrichment Service",
    "tags": ["data-processing", "enrichment"],
    "style": "dark",
    "timezone": "browser",
    "refresh": "30s",
    "time": {
      "from": "now-1h",
      "to": "now"
    },
    "panels": [
      {
        "id": 1,
        "title": "Service Health",
        "type": "stat",
        "targets": [
          {
            "expr": "enrichment_service_health_status",
            "legendFormat": "Health Status"
          }
        ],
        "fieldConfig": {
          "defaults": {
            "color": {
              "mode": "thresholds"
            },
            "thresholds": {
              "steps": [
                {"color": "red", "value": 0},
                {"color": "green", "value": 1}
              ]
            }
          }
        },
        "gridPos": {"h": 8, "w": 6, "x": 0, "y": 0}
      },
      {
        "id": 2,
        "title": "Messages Processed",
        "type": "stat",
        "targets": [
          {
            "expr": "enrichment_service_messages_processed_total",
            "legendFormat": "Total Messages"
          }
        ],
        "gridPos": {"h": 8, "w": 6, "x": 6, "y": 0}
      },
      {
        "id": 3,
        "title": "Processing Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(enrichment_service_messages_processed_total[5m])",
            "legendFormat": "{{status}}"
          }
        ],
        "yAxes": [
          {
            "label": "Messages/sec",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 0}
      },
      {
        "id": 4,
        "title": "Metadata Lookup Performance",
        "type": "graph",
        "targets": [
          {
            "expr": "histogram_quantile(0.50, rate(enrichment_service_processing_duration_seconds_bucket{message_type=\"metadata_lookup\"}[5m]))",
            "legendFormat": "50th percentile"
          },
          {
            "expr": "histogram_quantile(0.95, rate(enrichment_service_processing_duration_seconds_bucket{message_type=\"metadata_lookup\"}[5m]))",
            "legendFormat": "95th percentile"
          }
        ],
        "yAxes": [
          {
            "label": "Seconds",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 0, "y": 8}
      },
      {
        "id": 5,
        "title": "Cache Hit Rate",
        "type": "graph",
        "targets": [
          {
            "expr": "rate(enrichment_service_cache_hits_total[5m]) / (rate(enrichment_service_cache_hits_total[5m]) + rate(enrichment_service_cache_misses_total[5m]))",
            "legendFormat": "Cache Hit Rate"
          }
        ],
        "yAxes": [
          {
            "label": "Ratio",
            "min": 0,
            "max": 1
          }
        ],
        "gridPos": {"h": 8, "w": 12, "x": 12, "y": 8}
      },
      {
        "id": 6,
        "title": "Database Connection Pool",
        "type": "graph",
        "targets": [
          {
            "expr": "enrichment_service_active_connections{connection_type=\"postgres\"}",
            "legendFormat": "PostgreSQL Connections"
          },
          {
            "expr": "enrichment_service_active_connections{connection_type=\"redis\"}",
            "legendFormat": "Redis Connections"
          }
        ],
        "yAxes": [
          {
            "label": "Connections",
            "min": 0
          }
        ],
        "gridPos": {"h": 8, "w": 24, "x": 0, "y": 16}
      }
    ]
  }
}

